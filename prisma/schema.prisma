datasource db {
  provider = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Watchlist {
  id        String  @id @default(cuid())
  userId    String
  movieId   Int
  createdAt DateTime @default(now())

  @@unique([userId, movieId])
}
